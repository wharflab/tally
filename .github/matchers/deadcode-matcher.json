{
  "problemMatcher": [
    {
      "owner": "deadcode",
      "pattern": [
        {
          "regexp": "^([^:]+):(\\\\d+):(\\\\d+):\\\\s+unreachable\\\\s+func:\\\\s+(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ],
      "severity": "warning"
    }
  ]
}
